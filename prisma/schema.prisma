generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Template {
  id        String   @id @default(cuid())
  name      String
  prompt    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
